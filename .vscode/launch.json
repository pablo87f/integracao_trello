    "version": "0.2.0",
    "configurations": [
        {

            "cwd": "${workspaceRoot}",
            "env": {
                "NODE_ENV": "production",
                "REDIS_HOST": "10.8.0.31",
                "POSTGRES_HOST": "localhost"
            },
            "console": "internalConsole",
            "name": "Debug",
            "outFiles": ["${workspaceRoot}/build/**/*.js"],
            "preLaunchTask": "build",
            "request": "launch",
            "sourceMaps": true,
            "stopOnEntry": false,
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/app.ts"
        }
    ]
}